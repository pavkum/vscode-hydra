{
	"name": "hydra",
	"displayName": "hydra",
	"description": "An extension to chain commands",
	"version": "0.0.1",
	"publisher": "pavkuml",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:hydra.show"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "hydra.show",
				"title": "Hydra show command"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.30",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"colors": "^1.3.3"
	}
}
